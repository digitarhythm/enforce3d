#!/bin/bash

CWD=`pwd`
ENF=$(cd $(dirname $0)/..;pwd)
PROJ=$CWD

# フレームワークファイルのコピー
if [ ! -f $PROJ/index.php ]; then
	echo "ERROR: This project is static mode."
	exit
fi

cp -af $ENF/index.php $PROJ/
cp -af $ENF/extlib/* $PROJ/extlib/
coffee -b -j $PROJ/sysobject/enforce.core.enchant.js -c $ENF/lib/_originObject.coffee $ENF/lib/_stationary.coffee $ENF/lib/library.coffee $ENF/lib/main.coffee
